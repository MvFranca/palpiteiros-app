{
  "extends": "expo/tsconfig.base",
  
  "compilerOptions": {
    "jsx": "react-native",
    "module": "esnext", // ou "commonjs" dependendo do seu ambiente
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node",
    "target": "esnext", // ou a vers√£o do seu target do JavaScript
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "allowJs": true,
    "types": ["react", "react-native"]
  },
  "include": ["src"]
}
